{
  "name": "coffee-shell",
  "description": "CoffeeScript Shell",
  "preferGlobal": "true",
  "keywords": [
    "coffeescript",
    "shell"
  ],
  "maintainers": [
    "cobells <cobells@tumbledesign.com>",
    "dube <dube@tumbledesign.com>"
  ],
  "author": "Cobells & Dube",
  "version": "0.12.0",
  "license": "MIT",
  "engines": {
    "node": "0.6.x"
  },
  "main": "./src/coffee-shell",
  "bin": {
    "coffee-shell": "./bin/coffee-shell"
  },
  "homepage": "http://coffeescript.org",
  "repository": {
    "type": "git",
    "url": " git://github.com:markdube/coffee-shell.git"
  },
  "dependencies": {
    "coffee-script": ">=1.2.0",
    "colors": "*",
    "fibers": ">=0.6.4"
  },
  "devDependencies": {},
  "optionalDependencies": {},
  "config": {
    "aliases": {
      "ls": "ls --color=auto",
      "l": "ls -latr",
      "grep": "grep --color=auto",
      "egrep": "egrep --color=auto",
      "fgrep": "fgrep --color=auto"
    }
  }
}